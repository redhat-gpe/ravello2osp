  {{name}}_services_sg:
    type: OS::Neutron::SecurityGroup
    properties:
      name: {{bpname}}_{{name}}_services_sg
      rules:
      {% for rule in rules %}
      - {port_range_max: {{ rule.max }}, port_range_min: {{ rule.min }}, protocol: {{ rule.proto }}, remote_ip_prefix: {{ rule.remote_ip }}}
      {% endfor %}

